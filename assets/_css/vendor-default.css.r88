/* NAVBAR /
.navbar-inverse {
	background-color: #222;
	border-color: #080808;
}
.navbar-fixed-top {
	top: 0;
	z-index: 1000000;
	border-radius: 0;
	-webkit-border-radius: 0;
	-moz-border-radius: 0;
	-o-border-radius: 0;
	position: fixed;
	right: 0;
	left: 0;
	border-width: 0 0 1px;
}

.navbar {
	border-radius: 4px;
	position: relative;
	z-index: 900000;
	min-height: 50px;
	margin-bottom: 20px;
	border: 1px solid transparent;
}

/* ADDED FONT FORM RELIC CSS */
@font-face { 
	font-family: Ubuntu; 
	src: url('../fonts/Ubuntu-R.ttf'); 
}
/* HEADER */
body {
	font-family: Ubuntu !important;
	padding-top: 120px;
	position: relative;
}

.bs-header {
	font-size: 21px;
	text-align: left;
	padding: 30px 15px 40px;
	color: #cdbfe3;
	background-color: #FFB838;
	position: static;
	text-shadow: 0 1px 0 rgba(0,0,0,.15);
}

.bs-header h1 {
	margin-right: 380px;
	font-size: 60px;
	line-height: 1;
	color: #fff;
}

.bs-header p {
	margin-right: 380px;
	line-height: 1.5;
}
.sm-header {
	font-size: 21px;
	text-align: left;
	padding: 15px 15px 15px;
	color: #cdbfe3;
	background-color: #FFB838;
	position: static;
	text-shadow: 0 1px 0 rgba(0,0,0,.15);
}

.sm-header h1 {
	margin-right: 380px;
	font-size: 36px;
	line-height: 1;
	color: #fff;
}

.txt-blue1 {
	color: #38C5E3;
}
.bck-blue1 {
	background-color: #38C5E3;
}
.brd-blue1 {
	border-color: #38C5E3;
}
.txt-blue2 {
	color: #62CDDC;
}
.bck-blue2 {
	background-color: #62CDDC;
}
.brd-blue2 {
	border-color: #62CDDC;
}
